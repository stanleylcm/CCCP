@{
    ViewBag.Title = "Home Page";
}

<section class="content-header">
    <h1>
        <i class="fa fa-dashboard"></i> Dashboard
    </h1>
</section>

<section class="content">
    <div class="row">
        <div class="col-lg-6">
            <!-- small box -->
            <div class="small-box bg-green">
                <div class="icon">
                    <i class="glyphicon glyphicon-align-left"></i>
                </div>
                <div class="inner">
                    <p>General Enquiries</p>
                    <h3>14/20</h3>
                    <div class="progress xs">
                        <div class="progress-bar progress-bar-green" style="width: 70%;"></div>
                    </div>
                    <p>70% Finished</p>
                </div>
            </div>
        </div>
        <div class="col-lg-6">
            <!-- small box -->
            <div class="small-box bg-orange">
                <div class="icon">
                    <i class="glyphicon glyphicon-align-left"></i>
                </div>
                <div class="inner">
                    <p>Incidents</p>
                    <h3>28/40</h3>
                    <div class="progress xs">
                        <div class="progress-bar progress-bar-green" style="width: 70%;"></div>
                    </div>
                    <p>70% Finished</p>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-6">
            Outstanding General Enquiries
        </div>
        <div class="col-lg-6">
            Outstanding Incidents
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            Incident Summary
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            Crisis Approval List
        </div>
    </div>
</section>
